<Window x:Class="KrysenkoControl9.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KrysenkoControl9"
        mc:Ignorable="d"
        Title="Главное окно" Height="500" Width="960" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="FrameMainMenu" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="FrameMainBody" Grid.Column="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
